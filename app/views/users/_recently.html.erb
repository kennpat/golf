<table id="upcoming_table" class="tablesorter">
  <thead>
    <tr>
      <th>Date</th>
      <th>Course</th>
      <th>Tee Time</th>
    </tr>
  </thead>
  <tbody>
    <% @user.not_scored_rounds.each do |not_scored| %>
      <tr>
        <td><%= not_scored.round_date.strftime("%m/%d/%Y") %></td>
        <td><%= not_scored.course.name %></td>
        <td>
          <%= link_to "#{not_scored.tee_time.strftime("%I:%M%p")}", round_path(not_scored) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
